const BuscaSimulada = () => {
    return new Promise(resolve => {
        const min = 1;
        const max = 3;
        const rand = Math.floor(Math.random() * (max - min + 1)) + min;

        setTimeout(() => {
            resolve();
        }, rand * 1000);
    });
};

const init = () => {
    BuscaSimulada().then(() => {
        console.log("Busca simulada conclu√≠da");
    });
};

init();
