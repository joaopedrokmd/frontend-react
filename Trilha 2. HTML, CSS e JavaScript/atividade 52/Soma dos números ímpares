function somaNumerosImpares(numeros) {
    let soma = 0;

    numeros.forEach(function(numero) {
        if (numero % 2 !== 0) {
            soma += numero;
        }
    });

    return soma;
}

// Exemplo de uso - n√£o modifique
console.log(somaNumerosImpares([15, 5, 10])); // 20
console.log(somaNumerosImpares([2, 3, 4, 5, 6])); // 8
console.log(somaNumerosImpares([-2, -3, 4, 5, 6])); // 2
