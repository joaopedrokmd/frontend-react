function concatenaPrimeirasLetrasDoNome(primeiraLetraDoPrimeiroNome, primeiraLetraDoSobrenome) {
  return primeiraLetraDoPrimeiroNome + primeiraLetraDoSobrenome;
}


// Exemplo de uso - n√£o modifique
console.log(concatenaPrimeirasLetrasDoNome("J", "D")); // "JD"
console.log(concatenaPrimeirasLetrasDoNome("S", "B")); // "SB"