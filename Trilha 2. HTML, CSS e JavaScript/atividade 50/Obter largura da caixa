    <main class="container">

        <div id="box1" class="box">Caixa 1</div>
        <div id="box2" class="box">Caixa 2</div>
    </main>
    <script>
      
      function obterLarguraDaCaixa(valor) {
          return Number.parseInt(valor, 10);
}


        const box1 = document.querySelector("#box1");
        const box2 = document.querySelector("#box2");

        const box1Valor = getComputedStyle(box1).width;
        const box2Valor = getComputedStyle(box2).width;

        console.log("Primeiro largura da caixa", obterLarguraDaCaixa(box1Valor));
        console.log("Segundo largura da caixa", obterLarguraDaCaixa(box2Valor));
    </script>
  </body>
