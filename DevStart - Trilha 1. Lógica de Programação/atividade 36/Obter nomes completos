function obterNomesCompletos(usuarios) {
    return usuarios.map(function(usuario) {
        return usuario.nome + " " + usuario.sobrenome;
    });
}

let usuarios = [
  {
    nome: "BÃ¡rbara",
    sobrenome: "Silva",
    idade: 21,
  },
  {
    nome: "Carlos",
    sobrenome: "Bonaparte",
    idade: 38,
  }
];

console.log(obterNomesCompletos(usuarios));