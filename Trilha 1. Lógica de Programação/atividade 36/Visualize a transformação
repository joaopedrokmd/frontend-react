function obterTitulosDasReceitas(receitas) {
    return receitas.map(function(receita) {
        return receita.titulo;
    });
}

let receitas = [
    {
        titulo: "Macarr√£o",
        preparacaoEmMinutos: 30,
        classificacao: 4.5
    },
    {
        titulo: "Lasanha",
        preparacaoEmMinutos: 45,
        classificacao: 3.9
    }
];

console.log(obterTitulosDasReceitas(receitas));