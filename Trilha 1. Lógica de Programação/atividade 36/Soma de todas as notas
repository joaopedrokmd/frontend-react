function obterSomaDeTodasAsNotas(notas) {
    let soma = 0;

    for (let i = 0; i < notas.length; i++) {
        soma += notas[i].nota;
    }

    return soma;
}

let notas = [
  {
    nome: "Sandra Assis",
    nota: 8
  },
  {
    nome: "Ã‰rica Blanck",
    nota: 3
  }
];

console.log(obterSomaDeTodasAsNotas(notas));